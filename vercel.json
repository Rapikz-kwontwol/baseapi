{
  "version": 2,
  "routes": [
    { "src": "/api", "dest": "/api" },
    { "src": "/", "dest": "/docs.html" },
    { "src": "/docs", "dest": "/docs.html" },
    { "src": "/docs.html", "dest": "/docs.html" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/404.html", "status": 404 }
  ]
}
